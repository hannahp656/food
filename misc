garlic butter green beans
{
    "title": "Garlic Butter Green Beans",
    "image": "/food/recipe_images/garlic-butter-green-beans.jpg",
    "cover": "/food/recipe_images/garlic-butter-green-beans.jpg",
    "link": "/food/recipes/recipe-garlic-butter-green-beans.html",
    "tags": ["Side Dish", "15 min", "4 servings", "Vegetarian", "Gluten-Free"],
    "ingredients": [
    "1 lb fresh green beans, trimmed",
    "2 tbsp butter",
    "3 cloves garlic, minced",
    "Salt and pepper to taste",
    "1 tsp lemon juice (optional)"
    ],
    "instructions": [
    "Bring a pot of salted water to a boil. Add the green beans and cook for 3-4 minutes until tender-crisp. Drain and set aside.",
    "In a large skillet, melt the butter over medium heat. Add the minced garlic and sauté for about 30 seconds until fragrant.",
    "Add the cooked green beans to the skillet and toss to coat them in the garlic butter.",
    "Season with salt and pepper to taste. If desired, drizzle with lemon juice for added brightness.",
    "Cook for an additional 2 minutes, stirring occasionally, until heated through.",
    "Remove from heat and serve hot."
    ]
}
beets
{
    "title": "Roasted Beets",
    "image": "/food/recipe_images/roasted-beets.jpg",
    "cover": "/food/recipe_images/roasted-beets.jpg",
    "link": "/food/recipes/recipe-roasted-beets.html",
    "tags": ["Side Dish", "45 min", "4 servings", "Vegetarian", "Gluten-Free"],
    "ingredients": [
    "4 medium beets, washed and trimmed",
    "2 tbsp olive oil",
    "1 tsp salt",
    "1/2 tsp black pepper",
    "1 tbsp balsamic vinegar (optional)"
    ],
    "instructions": [
    "Preheat the oven to 400°F (200°C).",
    "Wrap each beet individually in aluminum foil.",
    "Place the wrapped beets on a baking sheet and roast in the preheated oven for 40-45 minutes, or until they are tender when pierced with a fork.",
    "Remove the beets from the oven and let them cool slightly.",
    "Unwrap the beets and peel off the skins using your hands or a paper towel.",
    "Slice or cube the beets and toss them with olive oil, salt, black pepper, and balsamic vinegar if using.",
    "Serve warm or at room temperature."
    ]
}
